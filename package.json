{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.2.1",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "bin": "heroku",
    "node": "7.5.0",
    "default_command": "dashboard",
    "s3": {
      "bucket": "heroku-cli-assets",
      "host": "cli-assets.heroku.com"
    },
    "plugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-fork",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-status"
    ]
  },
  "dependencies": {
    "cli-engine": "1.0.4",
    "heroku-apps": "2.1.25",
    "heroku-certs": "1.1.26",
    "heroku-cli-addons": "1.2.8",
    "heroku-cli-oauth": "2.0.6",
    "heroku-fork": "4.1.14",
    "heroku-git": "2.5.10",
    "heroku-local": "5.1.8",
    "heroku-orgs": "1.5.5",
    "heroku-pg": "2.0.16",
    "heroku-pipelines": "1.3.1",
    "heroku-redis": "1.2.8",
    "heroku-run": "3.4.3",
    "heroku-spaces": "2.6.0",
    "heroku-status": "3.0.5"
  },
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "repository": "https://github.com/heroku/cli",
  "scripts": {
    "build": "cli-build",
    "release": "cli-release"
  }
}
